/* pages/home/home.wxss */
page{
  background-color: #f3f5f5;
  padding-top: 5px;
}
.babys{
  height: 60px;
  border-top:1px solid #f3f5f5;
  background-color: #fff;
  position: relative;
}
.left{
  height: 100%;
  width: 80%;
  padding-top: 5px;
  padding-left: 5px;
  float: left;
}
.arrow{
  height: 16px;
  width: 16px;
  position: absolute;
  right: 5px;
  top: 50%;
  margin-top: -8px;
}
.photo{
  display: block;
  height: 40px;
  width: 40px;
  border-radius: 50%;
  float: left;
}
.info{
  float: left;
  width: 150px;
  height: 40px;
  margin-left: 5px;
}
.up{
  height: 20px;
  line-height: 20px;
  font-size: 30rpx;
  color: #888;
}
.up image{
  display: block;
  height: 20px;
  width: 20px;
  margin-right: 4px;
  float: left;
}
.down{
  height: 20px;
  line-height: 20px;
  font-size: 25rpx;
  color: #888;
}
.down image{
  display: block;
  height: 12px;
  width: 12px;
  margin: 4px;
  float: left;
}
.down text{
  float: left
}


.noBaby{
  padding-top: 20%;
}
.noBaby image{
  display: block;
  width: 180px;
  height: 170px;
  margin: 0 auto;
}
.noBaby view{
  text-align: center;
  color: #777;
  margin-top: 30px;
}
